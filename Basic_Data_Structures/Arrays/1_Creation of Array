#include<iostream>

using namespace std;

int main()
{
	int size;
	cout<<"Enter the length of your array :- ";
	cin>>size;
	cout<<endl;
	
	int array[size];
	
	for(int i=0 ; i<size ; i++)
	{
		cout<<"Enter element inside your array :- ";
		cin>>array[i];
	}
	
	cout<<endl;
	
	cout<<"Elements of your array are :- "<<endl;
	
	for(int i=0; i<size ; i++)
	{
		cout<<"Element at position "<<i<<" is :- "<<array[i]<<endl;
	}
	
	return 0;
}
